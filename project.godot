[application]

config/name="Godot Wild Jam 58"
run/main_scene="res://src/scenes/SceneManager.tscn"
config/icon="res://icon.png"

[autoload]

State="*res://src/State.gd"

[display]

window/vsync/use_vsync=false
window/per_pixel_transparency/allowed=true

[global]

clear=false
